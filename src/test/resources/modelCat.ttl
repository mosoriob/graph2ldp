#################################################################
#    Object Properties
#################################################################

###  http://schema.org/geo
<http://schema.org/geo> rdf:type owl:ObjectProperty .


###  https://w3id.org/okn/o/sd#adjustableParameter
<https://w3id.org/okn/o/sd#adjustableParameter> rdf:type owl:ObjectProperty ;
                                                rdfs:domain <https://w3id.org/okn/o/sd#ConfigurationSetup> ;
                                                rdfs:range <https://w3id.org/okn/o/sd#Parameter> ;
                                                rdfs:comment "Parameter that can be adjusted in a configuration setup"@en ;
                                                rdfs:label "adjustable parameter"@en .


###  https://w3id.org/okn/o/sd#adjustsVariable
<https://w3id.org/okn/o/sd#adjustsVariable> rdf:type owl:ObjectProperty ,
                                                     owl:FunctionalProperty ;
                                            rdfs:domain <https://w3id.org/okn/o/sd#Parameter> ;
                                            rdfs:range <http://www.geoscienceontology.org/geo-upper#Variable> ;
                                            rdfs:comment "Property that links parameter with the variable they adjust. This property can be used when parameters quantify variables without directly representing them. For example, a \"fertilizer percentage adjustment\" parameter can quantify a \"fertilizer price\" variable"@en ;
                                            rdfs:label "adjusts variable"@en .


###  https://w3id.org/okn/o/sd#hasConfiguration
<https://w3id.org/okn/o/sd#hasConfiguration> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <https://w3id.org/okn/o/sd#SoftwareVersion> ;
                                             rdfs:range <https://w3id.org/okn/o/sd#SoftwareConfiguration> ;
                                             rdfs:comment "Property that links a model to one of its configurations. A model may have multiple configurations, each of which is unique in terms of the inputs and outputs it uses."@en ;
                                             rdfs:label "has configuration"@en .


###  https://w3id.org/okn/o/sd#hasFixedResource
<https://w3id.org/okn/o/sd#hasFixedResource> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <https://w3id.org/okn/o/sd#DatasetSpecification> ;
                                             rdfs:range <https://w3id.org/okn/o/sd#SampleResource> ;
                                             rdfs:comment "Property that links a parameter or an input to a fixed value. For example, in a given configuration a parameter with the planting date for a model could be fixed to avoid the user changing it for that region."@en ;
                                             rdfs:label "has fixed resource"@en .


###  https://w3id.org/okn/o/sd#hasInput
<https://w3id.org/okn/o/sd#hasInput> rdf:type owl:ObjectProperty ;
                                     rdfs:domain <https://w3id.org/okn/o/sd#SoftwareConfiguration> ;
                                     rdfs:range <https://w3id.org/okn/o/sd#DatasetSpecification> ;
                                     rdfs:comment "Property that links a model configuration to the input types expected by it."@en ;
                                     rdfs:label "has input"@en .


###  https://w3id.org/okn/o/sd#hasOutput
<https://w3id.org/okn/o/sd#hasOutput> rdf:type owl:ObjectProperty ;
                                      rdfs:domain <https://w3id.org/okn/o/sd#SoftwareConfiguration> ;
                                      rdfs:range <https://w3id.org/okn/o/sd#DatasetSpecification> ;
                                      rdfs:comment "Property that expresses what are the outputs of a model"@en ;
                                      rdfs:label "has output"@en .


###  https://w3id.org/okn/o/sd#hasParameter
<https://w3id.org/okn/o/sd#hasParameter> rdf:type owl:ObjectProperty ;
                                         rdfs:domain <https://w3id.org/okn/o/sd#SoftwareConfiguration> ;
                                         rdfs:range <https://w3id.org/okn/o/sd#Parameter> ;
                                         rdfs:comment "Property that indicates the parameters of a model configuration"@en ;
                                         rdfs:label "has parameter"@en .


###  https://w3id.org/okn/o/sd#hasPresentation
<https://w3id.org/okn/o/sd#hasPresentation> rdf:type owl:ObjectProperty ;
                                            rdfs:domain [ rdf:type owl:Class ;
                                                          owl:unionOf ( <https://w3id.org/okn/o/sd#DatasetSpecification>
                                                                        <https://w3id.org/okn/o/sd#Parameter>
                                                                      )
                                                        ] ;
                                            rdfs:range <https://w3id.org/okn/o/sd#VariablePresentation> ;
                                            rdfs:comment "Property that links an instance of a dataset (or a dataset specification) to the presentation of a variable contained (or expected to be contained) on it."@en ;
                                            rdfs:label "has presentation"@en .


###  https://w3id.org/okn/o/sd#hasSetup
<https://w3id.org/okn/o/sd#hasSetup> rdf:type owl:ObjectProperty ;
                                     rdfs:domain <https://w3id.org/okn/o/sd#SoftwareConfiguration> ;
                                     rdfs:range <https://w3id.org/okn/o/sd#ConfigurationSetup> ;
                                     rdfs:comment "Property used to define configurations with some fixed resources and values. The rationale of this property is to allow predefined configurations"@en ;
                                     rdfs:label "has setup"@en .


###  https://w3id.org/okn/o/sd#hasSoftwareImage
<https://w3id.org/okn/o/sd#hasSoftwareImage> rdf:type owl:ObjectProperty ,
                                                      owl:FunctionalProperty ;
                                             rdfs:domain <https://w3id.org/okn/o/sd#SoftwareConfiguration> ;
                                             rdfs:range <https://w3id.org/okn/o/sd#SoftwareImage> ;
                                             rdfs:comment "Function to link a function with its corresponding container"@en ;
                                             rdfs:label "has software image"@en .


###  https://w3id.org/okn/o/sd#hasStandardVariable
<https://w3id.org/okn/o/sd#hasStandardVariable> rdf:type owl:ObjectProperty ,
                                                         owl:FunctionalProperty ;
                                                rdfs:range <https://w3id.org/okn/o/sd#StandardVariable> ;
                                                rdfs:comment "the standard name of a variable"@en ;
                                                rdfs:label "has standard variable"@en .


###  https://w3id.org/okn/o/sd#hasVersion
<https://w3id.org/okn/o/sd#hasVersion> rdf:type owl:ObjectProperty ;
                                       rdfs:domain <https://w3id.org/okn/o/sd#Software> ;
                                       rdfs:range <https://w3id.org/okn/o/sd#SoftwareVersion> ;
                                       rdfs:label "has software version"@en .


###  https://w3id.org/okn/o/sd#usesUnit
<https://w3id.org/okn/o/sd#usesUnit> rdf:type owl:ObjectProperty ,
                                              owl:FunctionalProperty ;
                                     rdfs:domain [ rdf:type owl:Class ;
                                                   owl:unionOf ( <https://w3id.org/okn/o/sd#Parameter>
                                                                 <https://w3id.org/okn/o/sd#VariablePresentation>
                                                               )
                                                 ] ;
                                     rdfs:range <http://qudt.org/schema/qudt/Unit> ;
                                     rdfs:comment "Property used to link a variable presentation or time interval to the unit they are represented in"@en ;
                                     rdfs:label "uses unit"@en .


###  https://w3id.org/okn/o/sdm#geo
:geo rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf <http://schema.org/geo> ;
     rdfs:label "geo"@en .


###  https://w3id.org/okn/o/sdm#intervalUnit
:intervalUnit rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <https://w3id.org/okn/o/sd#usesUnit> ;
              rdfs:range <http://qudt.org/schema/qudt/Unit> ;
              rdfs:comment "Unit used in an interval (e.g., month)"@en ;
              rdfs:label "interval unit"@en .


