        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unique Text</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://businessontology.com/ontology/updatedOn -->

    <owl:DatatypeProperty rdf:about="https://businessontology.com/ontology/updatedOn">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The datetime when data was updated in the graph.</rdfs:comment>
