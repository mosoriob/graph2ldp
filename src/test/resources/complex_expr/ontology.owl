    <!-- https://businessontology.com/ontology/Account -->

    <owl:Class rdf:about="https://businessontology.com/ontology/Account">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Agreement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/categorizedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://businessontology.com/ontology/AccountCategory"/>
                            <rdf:Description rdf:about="https://businessontology.com/ontology/AccountStatus"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/conformsTo"/>
                <owl:someValuesFrom rdf:resource="https://businessontology.com/ontology/Contract"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/hasMagnitude"/>
                <owl:someValuesFrom rdf:resource="https://businessontology.com/ontology/Balance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/identifiedBy"/>
                <owl:someValuesFrom rdf:resource="https://businessontology.com/ontology/Identifier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/describedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/name"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An agreement having a balance, as in a bank account, or credit card account, or Accounts Receivable account.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Account</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/AccountCategory -->

    <owl:Class rdf:about="https://businessontology.com/ontology/AccountCategory">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Category"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Account Category</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/AccountStatus -->

    <owl:Class rdf:about="https://businessontology.com/ontology/AccountStatus">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Category"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category to record different status values for accounts.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Account Status</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/AccountType -->

    <owl:Class rdf:about="https://businessontology.com/ontology/AccountType">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Category"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Categories of Accounts between Parties</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Account Type</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/Address -->

    <owl:Class rdf:about="https://businessontology.com/ontology/Address">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Content"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/categorizedBy"/>
                <owl:someValuesFrom rdf:resource="https://businessontology.com/ontology/AddressType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A reference to a place (real or virtual) that can be located by some routing algorithm, and where messages or things can be sent to or retrieved from. E.g. PO Box or URL to a PDF file.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Address</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/AddressType -->

    <owl:Class rdf:about="https://businessontology.com/ontology/AddressType">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Category"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Address Type</rdfs:label>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category for different types of addresses e.g. Bill To, Ship To, Rent, Own etc.</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/Agreement -->

    <owl:Class rdf:about="https://businessontology.com/ontology/Agreement">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Commitment"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/hasDirectPart"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://businessontology.com/ontology/Obligation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/Area -->

    <owl:Class rdf:about="https://businessontology.com/ontology/Area">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Magnitude"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A measurement of two-dimensional space.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/AreaUnit -->

    <owl:Class rdf:about="https://businessontology.com/ontology/AreaUnit">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/ProductUnit"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/multiplicand"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://businessontology.com/ontology/DistanceUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/multiplier"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://businessontology.com/ontology/DistanceUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A unit of two-dimensional area, such as square inches or hectares.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area Unit</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/Artifact -->

    <owl:Class rdf:about="https://businessontology.com/ontology/Artifact">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/createdOn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/uniqueText"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/updatedOn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An intentional, person-made thing, which could be physical or content</rdfs:comment>
        <rdfs:label xml:lang="en">Artifact</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/Balance -->

    <owl:Class rdf:about="https://businessontology.com/ontology/Balance">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Magnitude"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/affectedBy"/>
                <owl:someValuesFrom rdf:resource="https://businessontology.com/ontology/Transaction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/BaseUnit -->

    <owl:Class rdf:about="https://businessontology.com/ontology/BaseUnit">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/SimpleUnitOfMeasure"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://businessontology.com/ontology/USDollar"/>
                    <rdf:Description rdf:about="https://businessontology.com/ontology/kilogram"/>
                    <rdf:Description rdf:about="https://businessontology.com/ontology/meter"/>
                    <rdf:Description rdf:about="https://businessontology.com/ontology/second"/>
                </owl:oneOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A primitive unit that cannot be decomposed into other units. It can be converted from one measurement system to another.  The base units in gist are the seven primitive units from the System Internationale (SI): (meter, second, kilogram, ampere, kelvin, mole, candela), plus three convenience ones: each. bit and usDollar.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Base Unit</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/Behavior -->

    <owl:Class rdf:about="https://businessontology.com/ontology/Behavior">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Category"/>
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Intention"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/allows"/>
                <owl:someValuesFrom rdf:resource="https://businessontology.com/ontology/Behavior"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A way of categorising events.  E.g., differentiating drilling versus cutting.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Behavior</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/Building -->

    <owl:Class rdf:about="https://businessontology.com/ontology/Building">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/Landmark"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/locatedAt"/>
                <owl:someValuesFrom rdf:resource="https://businessontology.com/ontology/Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/BundledCatalogItem -->

    <owl:Class rdf:about="https://businessontology.com/ontology/BundledCatalogItem">
        <rdfs:subClassOf rdf:resource="https://businessontology.com/ontology/CatalogItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://businessontology.com/ontology/hasDirectPart"/>
                <owl:someValuesFrom rdf:resource="https://businessontology.com/ontology/CatalogItem"/>
