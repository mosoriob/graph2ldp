<?xml version="1.0"?>
<rdf:RDF xmlns="https://businessontology.com/ontology/"
     xml:base="https://businessontology.com/ontology/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://businessontology.com/ontology"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://businessontology.com/ontology/actualEnd -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/actualEnd">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/end"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When something did end, therefore noting an historical event.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actual End</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/actualStart -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/actualStart">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/start"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When something did start, therefore noting an historical event.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actual Start</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/affectedBy -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/affectedBy">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Where the effect came from</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Affected By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/affects -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/affects">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The subject has or had or will have an effect on the object.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Affects</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/allocatedBy -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/allocatedBy">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://businessontology.com/ontology/Organization"/>
                    <rdf:Description rdf:about="https://businessontology.com/ontology/Person"/>
                    <rdf:Description rdf:about="https://businessontology.com/ontology/System"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connection between an ID and the thing that minted the ID.  It may be a person or organization, or could be an algorithm (next available or random number generator) or a system (e.g. for an External ID).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Allocated By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/allows -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/allows">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The intention (say a grant) allows a particular kind of activity (for instance egress)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Allows</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/basedOn -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/basedOn">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pointer to the thing something was derived from</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Based On</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/basisFor -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/basisFor">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reason for an event</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Basis For</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/categorizedBy -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/categorizedBy">
        <rdfs:range rdf:resource="https://businessontology.com/ontology/Category"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Points to a taxonomy item or other less formally defined class.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Categorized By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/characterizedAs -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/characterizedAs">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/Event"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/Behavior"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A way to categorize a behavior.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Characterized As</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/conformsTo -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/conformsTo">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The subject conforms to the Object, e.g. meet an obligation, meet terms of an offer, adhere to a specification.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Conforms To</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/connectedTo -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/connectedTo">
        <rdfs:comment>A non-owning, non-causal, non-subordinate (i.e., peer-to-peer) relationship.</rdfs:comment>
        <rdfs:label>Connected To</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/contributesTo -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/contributesTo">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The parts of a system contribute to the goal/ function of the whole system</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Contributes To</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/denominator -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/denominator">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/RatioUnit"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/UnitOfMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a RatioUnit such as meters/second to the denominator Unit (e.g. second).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Denominator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/directPartOf -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/directPartOf">
        <owl:inverseOf rdf:resource="https://businessontology.com/ontology/hasDirectPart"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relationship between a  part and a whole where the part has independent existence.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Direct Part Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/end -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/end">
        <rdfs:range rdf:resource="https://businessontology.com/ontology/TimeInstant"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connects the subject to its end time.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">End</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/geoDirectlyContainedIn -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/geoDirectlyContainedIn">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">E.g. the neighborhood is in the city</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geo Directly Contained In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/geoDirectlyContains -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/geoDirectlyContains">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The subject geospatially contains the object.  E.g. the area of a city contains the area of its neighborhoods</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geo Directly Contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/geoOccupiedBy -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/geoOccupiedBy">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">what is in the location</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geo Occupied By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/getter -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/getter">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/hasParty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The recipient.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Getter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/giver -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/giver">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/hasParty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The active party, the one with the obligation or the one initiating the transfer.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Giver</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasBaseUnit -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasBaseUnit">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EXAMPLE: saying that a furlong hasBaseUnit  meter says it is a DistanceUnit.</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a UnitOfMeasure to its BaseUnit.  This indicates what kind Unit something is, e.g. saying that a furlong hasBaseUnit  meter says it is a DistanceUnit.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Base Unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasBirthDate -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasBirthDate">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/actualStart"/>
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/LivingThing"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/TimeInstant"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Date a living thing was &quot;born&quot; (or germinated, for plants).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Birthdate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasDeathDate -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasDeathDate">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/actualEnd"/>
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/LivingThing"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/TimeInstant"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Date a living thing died</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Death Date</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasDirectPart -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasDirectPart">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/hasPart"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relationship between a whole and a part where the part has independent existence. NOTE: Use this property to directly associate parts. hasPart is the transitive version. NOTE: No cascading delete.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Direct Part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasJurisdiction -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasJurisdiction">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When laws and contracts are meted out.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Jurisdiction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasLocation -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasLocation">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/Building"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/Place"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasMagnitude -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasMagnitude">
        <rdfs:range rdf:resource="https://businessontology.com/ontology/Magnitude"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To have a comparable numerical value. Each magnitude has a unit.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Magnitude</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasOrder -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasOrder">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/customer"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/TransactionDocument"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Order</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasPart -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasPart">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relationship between a whole and a part where the part has independent existence.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasParty -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasParty">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://businessontology.com/ontology/Organization"/>
                    <rdf:Description rdf:about="https://businessontology.com/ontology/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The people or organizations participating in an agreement or obligation.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Party</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasPrecision -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasPrecision">
        <rdfs:range rdf:resource="https://businessontology.com/ontology/Magnitude"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EXAMPLE:  Temperature precise to tenth of a degree C; TimeInstant precise to 24 hours.</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links a Magnitude to the degree of accuracy of the numeric value.   This allows for fuzzy numbers.  All magnitudes have a precision.  Usually we don&apos;t record them.  When we do this, it will be a value whose extent covers 2 standard deviations around the stated magnitude</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NOTE:  Most frequently apples to Magnitude(s) and TimeInstant. Could also apply to a measurement.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Precision</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasStandardUnit -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasStandardUnit">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/UnitOfMeasure"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/CoherentUnit"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">For a complex unit refers to a unit that has all the component parts in SI</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Standard Unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasSubOrganization -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasSubOrganization">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents hierarchical containment of Organizations or OrganizationalUnits; indicates an organization which is a sub-part or child of this organization.

From: https://www.w3.org/TR/vocab-org/#org:hasSubOrganization</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Sub Organization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasTransactionDocument -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasTransactionDocument">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship between a Transaction Document Item and it&apos;s header Transaction Document.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Transaction Document</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasUnit -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasUnit">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indicates a unit which is part of this Organization, e.g. a Department within a larger Organization.

From: https://www.w3.org/TR/vocab-org/#org:hasUnit</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/hasUoM -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/hasUoM">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/Magnitude"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/UnitOfMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Which unit of measure you are using. All measures are expressed in some unit of measure, even if we don&apos;t know what it is initially.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has Unit of Measure</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/identifiedBy -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/identifiedBy">
        <rdfs:range rdf:resource="https://businessontology.com/ontology/Identifier"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is like a URI: a thing can have more than one ID, but each of the IDs must refer to a unique thing. Use &apos;Internal ID&apos; for ID&apos;s that were allocated by the Graph. Use &apos;External ID&apos; for ID&apos;s allocated by an outside entity e.g. an Application, a Person in a company etc. Use &apos;ID&apos; for ID&apos;s that correspond to real-world things e.g. name of a room.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Identified By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/locatedAt -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/locatedAt">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Located At</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/madeUpOf -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/madeUpOf">
        <rdfs:range rdf:resource="https://businessontology.com/ontology/PhysicalSubstance"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">as in the vase is made up of clay</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Made Up Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/multiplicand -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/multiplicand">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/ProductUnit"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/UnitOfMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a ProductUnit such as square mile to the second of two units multiplied together (e.g. mile).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Multiplicand</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/multiplier -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/multiplier">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/ProductUnit"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/UnitOfMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a ProductUnit such as square mile to the first of two units multiplied together (e.g. mile)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Multiplier</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/numerator -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/numerator">
        <rdfs:domain rdf:resource="https://businessontology.com/ontology/RatioUnit"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/UnitOfMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a RatioUnit such as meter(s)/second to the numerator Unit (e.g. meter).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Numerator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/offsetToUniversal -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/offsetToUniversal">
