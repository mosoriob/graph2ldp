        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Party For</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/permanentGeoOccupies -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/permanentGeoOccupies">
