        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NOTE: Our identity criteria require that something refers to each Time Instant instance.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Time Instant</rdfs:label>
    </owl:Class>
    


    <!-- https://businessontology.com/ontology/TimeZone -->

    <owl:Class rdf:about="https://businessontology.com/ontology/TimeZone">
