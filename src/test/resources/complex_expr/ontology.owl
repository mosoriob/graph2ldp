        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Permanent Geo Occupies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/planned -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/planned">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dates that were in the future at the time they were made.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Planned</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/plannedEnd -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/plannedEnd">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/end"/>
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/planned"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/TimeInstant"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A date/time that was at least at some point in time in the future. It may be in the past now, but when we planned it, it was in the future. NOTE:  Most frequently apples to Event(s) and Offer(s). E.g. a conference or sale offer.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Planned End</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/plannedStart -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/plannedStart">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/planned"/>
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/start"/>
        <rdfs:range rdf:resource="https://businessontology.com/ontology/TimeInstant"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A date/time that was at least at some point in time in the future. It may be in the past now, but when we planned it, it was in the future.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Planned Start</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/start -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/start">
        <rdfs:range rdf:resource="https://businessontology.com/ontology/TimeInstant"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connects the subject to its start time.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Start</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/theParty -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/theParty">
        <rdfs:subPropertyOf rdf:resource="https://businessontology.com/ontology/connectedTo"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://businessontology.com/ontology/Organization"/>
                    <rdf:Description rdf:about="https://businessontology.com/ontology/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Person or Organisation entity who is the point of Contact in a Contact Relationship to a Business Entity.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Party</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://businessontology.com/ontology/transactionItemFor -->

    <owl:ObjectProperty rdf:about="https://businessontology.com/ontology/transactionItemFor">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship between a Transaction Document Item and a Product or Service e.g. a sales order line for a specific product.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Transaction Item For</rdfs:label>
    </owl:ObjectProperty>
    


